<h2>Users finished the most tasks:</h2>
<div class="row g-3">
  <% for (let user of users) { %>

    <div class="col-4">
      <div class="card p-1">
        <h5 class="card-title"><%="First Name: " + user.firstName %></h5>
        <div class="card-text">
          <div><%= "Last Name: " + user.lastName %></div>
          <div><%= "Email: " + user.email %></div>
          <div><%= "ID: " + user.id %></div>
          <div><%= "Number of Accomplished Tasks: " + user.numberOfAccomplishedTask %></div>


          <div>
            <a class="btn btn-outline-primary" href="<%= '/users/' + user.id + '/editt????' %>">Edit</a>
            <a class="btn btn-outline-danger" href="<%= '/users/' + user.id + '/delete???' %>">Delete</a>
          </div>



          
        </div>
      </div>
    </div>


  <% } %>



</div>
